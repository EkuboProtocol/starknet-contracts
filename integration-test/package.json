{
  "name": "@ekubo/contracts-integration-test",
  "private": true,
  "version": "0.0.0",
  "description": "Integration tests for the core contracts of Ekubo Protocol",
  "scripts": {
    "devnet": "starknet-devnet --hide-server-logs --seed 0",
    "test": "start-server-and-test --expect 404 devnet http://127.0.0.1:5050 \"jest\"",
    "create-dump": "start-server-and-test --expect 404 devnet http://127.0.0.1:5050 \"ts-node create-dump.ts\""
  },
  "author": "Moody Salem",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "decimal.js-light": "^2.5.1",
    "starknet": "^5.19.0",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}